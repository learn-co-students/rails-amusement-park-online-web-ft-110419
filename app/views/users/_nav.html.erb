<% if logged_in? %>
    <%= link_to  "#{current_user.name}\'s profile #{"ADMIN" if current_user.admin}", nil, class: 'navbar-brand'%>
    <%= link_to  'Log Out', '/users', method: 'delete', class: 'navbar-brand'%>
<% else %>
    <%= link_to  'Sign Up', '/users/new', class: 'navbar-brand'%>
    <%= link_to  'Log In', '/signin', class: 'navbar-brand'%>
<% end %>
